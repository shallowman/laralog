{
    "name": "shallowman/laralog",
    "description": "A middleware for Laravel framework which is a logging tool,that can capture every one HTTP request and record the detailsã€‚The middleware default formatter is json formatter.",
    "keywords": [
        "laravel",
        "log",
        "json",
        "filebeat"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Feron Wu",
            "email": "feron.wu@gmail.com"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/log": ">=5.6",
        "illuminate/http": ">=5.6",
        "illuminate/support": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "Shallowman\\Laralog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shallowman\\Laralog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shallowman\\Laralog\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "roave/security-advisories": "dev-master"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
